	# ----------- CONFIGURATION ----------------
	
	# Default home dir
	HM_DIR="/usr/local/picuntu"
	# Set debug mode
	DBUG="FALSE"
	
	HIST_FL="$HM_DIR/install.history"
	
	# Set logging
	PLOG_FL="/tmp/picuntu_install.log"
	PLOG="TRUE"
	[ $PLOG == "TRUE" ] && touch $PLOG_FL
	# DPKG_PURGE="dpkg --list |grep "^rc" | cut -d " " -f 3 | xargs sudo dpkg --purge"
	
	FST_RN_FL="$HM_DIR/first-time-picuntu"
	
	# APT command
		if [ ! $DBUG == "TRUE" ] 
			then
				APT="/usr/bin/apt-get -y --purge remove "
			else		
				APT="echo 'debug: apt-get -y --purge remove '"
		fi
